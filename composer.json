{
  "name": "afrux/asirem",
  "description": "The weird Flarum theme made with potatoes as fuel, by Afrux.",
  "keywords": [
    "flarum"
  ],
  "type": "flarum-extension",
  "license": "MIT",
  "require": {
    "flarum/core": "^1.0.0",
    "sycho/flarum-uikit": "^0.1.0",
    "afrux/flarum-theme-base": "^0.1.0"
  },
  "require-dev": {
    "sycho/flarum-uikit": "0.1.x-dev",
    "afrux/flarum-theme-base": "0.1.x-dev"
  },
  "authors": [
    {
      "name": "Sami Mazouz",
      "email": "ilyasmazouz@gmail.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Afrux\\Asirem\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Afrux\\Asirem\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    },
    "flarum-extension": {
      "title": "Asirem",
      "category": "theme",
      "optional-dependencies": [
          "flarum/tags",
          "flarum/approval",
          "sycho/flarum-advanced-extension-categories"
        ],
      "icon": {
        "image": "icon.svg"
      }
    },
    "flagrow": {
      "discuss": ""
    }
  }
}
